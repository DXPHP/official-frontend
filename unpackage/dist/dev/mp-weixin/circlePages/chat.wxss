/* 胶囊*/
.tn-custom-nav-bar__back.data-v-1c08cc7e {
  width: 100%;
  height: 100%;
  position: relative;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  box-sizing: border-box;
  background-color: rgba(0, 0, 0, 0.15);
  border-radius: 1000rpx;
  border: 1rpx solid rgba(255, 255, 255, 0.5);
  color: #FFFFFF;
  font-size: 18px;
}
.tn-custom-nav-bar__back .icon.data-v-1c08cc7e {
  display: block;
  flex: 1;
  margin: auto;
  text-align: center;
}
.tn-custom-nav-bar__back.data-v-1c08cc7e:before {
  content: " ";
  width: 1rpx;
  height: 110%;
  position: absolute;
  top: 22.5%;
  left: 0;
  right: 0;
  margin: auto;
  -webkit-transform: scale(0.5);
          transform: scale(0.5);
  -webkit-transform-origin: 0 0;
          transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  opacity: 0.7;
  background-color: #FFFFFF;
}
.tn-tabbar-height.data-v-1c08cc7e {
  min-height: 100rpx;
  height: calc(120rpx + env(safe-area-inset-bottom) / 2);
}
/* 用户头像 start */
.logo-image.data-v-1c08cc7e {
  width: 80rpx;
  height: 80rpx;
  position: relative;
}
.logo-pic.data-v-1c08cc7e {
  background-size: cover;
  background-repeat: no-repeat;
  background-position: top;
  border-radius: 50%;
}
.bg-img-cont.data-v-1c08cc7e {
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
  height: 260rpx;
  width: 55vw;
  margin: 10rpx 0 0 0;
  border-radius: 12rpx;
}
